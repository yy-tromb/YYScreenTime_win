ID_MENU MENU
BEGIN
    POPUP "ファイル(&F)"
    BEGIN
        MENUITEM "設定(&S)", IDM_SETTING
        MENUITEM "終了(&E)", IDM_EXIT
    END
    POPUP "機能(&U)"
    BEGIN
        MENUITEM "フォーカスモード(&F)", IDM_FOCUSMODE
