        MENUITEM SEPARATOR
        MENUITEM "常駐(&R)", IDM_RESIDENT
        MENUITEM "スタートアップ(&S)", IDM_STARTUP
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "このアプリについて(&A)...", IDM_ABOUT
    END
END

